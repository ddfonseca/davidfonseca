<h1>David Fonseca</h1>

{% set postlist = collections.post | reverse %}
<ul>
    {% for post in postlist %}
        <li style="margin-bottom: 1rem; list-style: none;">
            <h2 style="margin: 0; font-size: 1.3rem;"><a href="{{ post.url }}">{{ post.data.title }}</a></h2>
        </li>
    {% endfor %}
</ul>